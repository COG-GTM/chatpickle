{
    "name": "chatpickle-cucumber-examples",
    "version": "1.0.0",
    "description": "Cucumber Implementation of Chat Pickle (Lex Conversational Flow Tests)",
    "keywords": [
        "BDD",
        "Chatbot",
        "Conversation",
        "Testing",
        "Cucumber",
        "AWS",
        "Amazon",
        "Lex"
    ],
    "author": "Aaron Arsenault",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "git://https://github.com/libertymutual/chatpickle"
    },
    "scripts": {
        "build": "rm -rf dist/* && npm run build:typescript && npm run build:other",
        "build:typescript": "tsc",
        "build:other": "cp -r src/cucumberSupport dist/cucumberSupport",
        "start": "npm run build && CHATPICKLE_CONSUMER_PATH='examples/parentProject/' node dist/index.js",
        "test": "echo 'TODO - Add Unit Tests to Project'",
        "lint": "eslint \"{scripts,src,test}/**/*.{js,ts}\"",
        "lint:fix": "eslint --fix \"{scripts,src,test}/**/*.{js,ts}\""
    },
    "dependencies": {
        "aws-sdk": "^2.597.0",
        "chai": "^4.2.0",
        "cucumber": "^6.0.5"
    },
    "devDependencies": {
        "@types/node": "^13.1.4",
        "@typescript-eslint/eslint-plugin": "^2.15.0",
        "@typescript-eslint/parser": "^2.15.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-prettier": "^3.1.2",
        "prettier": "^1.19.1",
        "typescript": "^3.7.4"
    },
    "engines": {
        "node": "8.10.0"
    }
}
